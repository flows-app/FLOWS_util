{
    "id": "FLOWS/util/timer",
    "lambda": "lambda-arn",
    "role": "iam-arn",
    "output": {
        "schema": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "description": "datetime",
            "type": "object",
            "properties": {
                "now": {
                    "format": "date-time",
                    "type": "string"
                }
            },
            "required": ["now"]
        },
        "examples": {
            "result": {
                "now": "2018-05-01T00:00:00Z"
            }
        }
    },
    "type": "event",
    "description": "triggers at a certain point in time",
    "icon": "https://flows.app/favicon.png",
    "billing": "standard"
}
